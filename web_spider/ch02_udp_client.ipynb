    "import socket\n",
    "\n",
    "s = socket.socket(socket.AF_INET, socket.SOCK_DGRAM)\n",
    "for data in [b'ABC', b'CDE', b'DEF']:\n",
    "    s.sendto(data, ('127.0.0.1',9999))\n",
    "    print(s.recv(1024).decode('utf-8'))\n",
    "s.close()"
